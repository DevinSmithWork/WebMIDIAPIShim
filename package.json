{
    "name": "web-midi-api-shim",
    "version": "0.1.0",
    "description": "Shim for the WebMIDI API, intended to be used in environments that don't have the WebMIDI API implemented: Nodejs and all browsers except Chrome, Chromium and Opera",
    "keywords": [
        "midi",
        "music",
        "sound",
        "webmidi api"
    ],
    "main": "node/index.js",
    "module": "src/index.js",
    "browser": "browser/WebMIDIAPI.js",
    "homepage": "https://github.com/cwilso/WebMIDIAPIShim",
    "bugs": "https://github.com/cwilso/WebMIDIAPIShim/issues",
    "license": "Apache-2.0",
    "author": {
        "name": "Chris Wilson",
        "email": "cwilso@gmail.com",
        "url": "http://cwilso.com/"
    },
    "contributors": [
        {
            "name": "jazz-soft",
            "email": "jazz-dev@jazz-soft.net",
            "url": "http://jazz-soft.net"
        },
        {
            "name": "abudaan",
            "email": "daniel@abumarkub.net",
            "url": "http://github.com/abudaan"
        }
    ],
    "directories": {
        "example": "examples"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cwilso/WebMIDIAPIShim.git"
    },
    "scripts": {
        "build": "browserify -t [babelify] src/index.js -o browser/WebMIDIAPI.js",
        "minify": "babel-minify browser/WebMIDIAPI.js > browser/WebMIDIAPI.min.js",
        "compile": "babel src --out-dir ./dist",
        "update": "npm run compile && npm run build && npm run minify",
        "watch": "watchify src/index.js -t babelify -o browser/WebMIDIAPI.js --debug --verbose"
    },
    "dependencies": {
        "jazz-midi": "^1.5.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-minify": "^0.2.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.24.1",
        "babelify": "^7.3.0",
        "browserify": "^14.4.0",
        "eslint": "^4.6.1",
        "eslint-config-airbnb-base": "^12.0.0",
        "eslint-plugin-import": "^2.7.0",
        "watchify": "^3.9.0"
    },
    "engines": {
        "node": "0.10.x"
    },
    "private": false
}
